{
    "manifest_version": 3,
    "version": "2.0.0",
    "name": "ДЕТЕЙ",
    "action": {
        "default_icon": {
            "128": "DETEY_128_128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "activeTab"
    ],
    "web_accessible_resources": [
        {
            "resources": ["DETEY.jpg"],
            "matches": ["<all_urls>"]
        }
    ]
}
