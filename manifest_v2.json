{
    "manifest_version": 2,
    "version": "2",
    "name": "ДЕТЕЙ",
    "browser_action": {
        "default_icon": "DETEY_128_128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}
