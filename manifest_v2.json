{
    "manifest_version": 2,
    "version": "1.0.1",
    "name": "ДЕТЕЙ",
    "browser_action": {
        "default_icon": "../DETEY.jpg"
    },
    "background": {
        "scripts": ["../background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["../content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}
